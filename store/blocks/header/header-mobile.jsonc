{
  "header-layout.mobile": {
    "title": "Header Mobile",
    "children": ["header-row#mob", "csstag"]
  },

  "header-row#mob": {
    "title": "Conte√∫do",
    "children": ["flex-layout.row#header-container-mob"],
    "props": {
      "zIndex": 5,
      "sticky": true,
      "fullWidth": true,
      "inverted": false
    }
  },
  "flex-layout.row#header-container-mob": {
    "children": [
      "flex-layout.row#header-mob-menu",
      "flex-layout.row#header-mob-logo",
      "flex-layout.row#header-mob-icons"
    ],
    "props": {
      "blockClass": "header-container-mob"
    }
  },

  "flex-layout.row#header-mob-menu": {
    // "children": ["MenuMobile"],
    "children": ["image#menu"],
    "props": {
      "blockClass": "mob-menu-search-container"
    }
  },

  "image#menu": {
    "props": {
      "src": "assets/svg/icon-menu-white.svg"
    }
  },

